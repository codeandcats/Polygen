{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
		"indent": [true, "tabs", 4],
        "arrow-parens": false,
        "interface-name": [true, "never-prefix"],
        "object-literal-sort-keys": false,
        "no-console": false,
        "quotemark": [true, "single"],
        "trailing-comma": false
    },
    "rulesDirectory": []
}