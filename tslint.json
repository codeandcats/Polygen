{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
		"indent": [true, "tabs", 4],
        "arrow-parens": false,
		"interface-name": [true, "never-prefix"],
		"max-classes-per-file": false,
		"member-ordering": false,
		"no-console": false,
		"no-empty-interface": false,
		"object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "trailing-comma": false
    },
    "rulesDirectory": []
}